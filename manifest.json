{
  "author": "Gopi Tyro",
  "browser_action": {
  "default_icon": "img/icon-16.png",
  "default_popup": "popup.html"
  },
  "short_name": "Amazon asin checker",
  "description": "Validate and fetch amazon product links by ASIN code",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "name": "Amazon ASIN Fetcher",
  "permissions": [ "activeTab", "storage","notifications","contextMenus" ],
  "version": "1.0.1"
}
